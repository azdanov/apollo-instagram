{
	"name": "apollo-instagram-app",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview",
		"test": "xo"
	},
	"dependencies": {
		"react": "^17.0.0",
		"react-dom": "^17.0.0"
	},
	"devDependencies": {
		"@vitejs/plugin-react-refresh": "^1.3.1",
		"eslint-config-xo-react": "^0.24.0",
		"eslint-plugin-react": "^7.23.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"prettier": "^2.2.1",
		"vite": "^2.1.3",
		"webstorm-xo-eslint-bridge": "^6.8.0",
		"xo": "^0.38.2"
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"envs": [
			"browser",
			"es2021"
		],
		"rules": {
			"node/file-extension-in-import": "off",
			"camelcase": "off",
			"unicorn/filename-case": [
				"error",
				{
					"cases": {
						"camelCase": true,
						"pascalCase": true
					}
				}
			]
		}
	}
}
